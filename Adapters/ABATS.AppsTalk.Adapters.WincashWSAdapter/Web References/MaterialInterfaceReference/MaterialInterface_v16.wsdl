    <xs:schema xmlns:ntsmas="http://jif.ntswincash.com/common/basetypes/masterdata/v12" xmlns:nts="http://jif.ntswincash.com/common/basetypes/v12" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:cusval="http://jif.ntswincash.com/common/customvalues/v12" xmlns:m2s="http://jif.ntswincash.com/entity/materialToStockMapping/v12" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://jif.ntswincash.com/entity/materialToStockMapping/v12" jaxb:version="2.0">
      <xs:import namespace="http://jif.ntswincash.com/common/basetypes/v12" />
      <xs:import namespace="http://jif.ntswincash.com/common/basetypes/masterdata/v12" />
      <xs:import namespace="http://jif.ntswincash.com/common/customvalues/v12" />
      <xs:element name="inventoryAssignments" type="m2s:materialToStockMappingList" />
      <xs:complexType name="materialToStockMappingList">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="m2s:materialToStockMapping" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="materialToStockMapping" type="m2s:materialToStockMapping" />
      <xs:complexType name="materialToStockMapping">
        <xs:annotation>
          <xs:documentation>Assignment between a material and a stock/inventory</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecord">
            <xs:sequence>
              <xs:choice minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Material reference - not required if the element is a child of a
                material element</xs:documentation>
                </xs:annotation>
                <xs:element name="materialNumber" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The article. References MATERIAL.MATNO.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="materialID" type="xs:integer">
                  <xs:annotation>
                    <xs:documentation>The article. References MATERIAL.GID.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:choice>
              <xs:choice>
                <xs:element name="stockName" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The inventory. References STOCK.STOCKNAM.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="stockID" type="xs:integer">
                  <xs:annotation>
                    <xs:documentation>The inventory. References STOCK.GID.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:choice>
              <xs:element name="isStandard" type="nts:flag">
                <xs:annotation>
                  <xs:documentation>This is the default sales inventory for the article. For one
                article, there can be only one MAT2STOCKMAPPING entry with element isStandard is
                'Yes'.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="valuationPrice" type="nts:nonFractionNumber">
                <xs:annotation>
                  <xs:documentation>The price for inventory valuation. When NULL,
                MATERIAL.VALUATIONPRICE is used.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="quantitySettings" type="m2s:stockQuantitySettings">
                <xs:annotation>
                  <xs:documentation>Contains several quantity limits for automatic disposition
                generation.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="usageSettings" type="m2s:stockUsageSettings">
                <xs:annotation>
                  <xs:documentation>Contains several flags to define for which business cases this stock
                assignment can be used (e.g. the item can be sold from this stock but cannot be
                returned to this stock)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="inventoryPickingCoordinates" type="m2s:inventoryPickingCoordinates" />
              <xs:element minOccurs="0" ref="cusval:customValues">
                <xs:annotation>
                  <xs:documentation>Optional customer specific values which are used to extends the
                interface.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="stockQuantitySettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="minimumQuantity" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Minimum stock level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="maximumQuantity" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Maximum stock level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="warningQuantity" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Warn stock level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="requiredQuantity" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Recorded stock level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="replenishmentQuantity" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Replenishment quantity used for automatic order suggestions
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="quantityCalculationType" type="m2s:quantityCalculationType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="stockUsageSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="showSale" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for sales</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showReturn" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for returns</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showOrder" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for orders</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showLeaseOut" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for lease transactions</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showLeaseIn" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for lease returns</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showLoanItemForRepair" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for repair manager postings with loan devices
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showSwapItemForRepair" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display for repair manager postings with swap devices
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showWaitList" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Display waitlist reservation for item in this stock. Default value is 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="inventoryPickingCoordinates">
        <xs:annotation>
          <xs:documentation>The picking coordinates define the location in stock of the material.
      </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="coordinateX" type="xs:string">
            <xs:annotation>
              <xs:documentation>Coordinate X</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="coordinateY" type="xs:string">
            <xs:annotation>
              <xs:documentation>Coordinate Y</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="coordinateZ" type="xs:string">
            <xs:annotation>
              <xs:documentation>Coordinate Z</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="coordinateN" type="xs:string">
            <xs:annotation>
              <xs:documentation>Coordinate N</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="quantityCalculationType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="MAX-MIN">
            <xs:annotation>
              <xs:documentation> Fill to maximum quantity (MAXQTY) when available stock is lower than
            minimum quantity (MINQTY).</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="REQ-WARN">
            <xs:annotation>
              <xs:documentation> Fill to required quantity (REQQTY) when available stock is lower than
            warn quantity (WARNQTY).</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="REQ-MIN">
            <xs:annotation>
              <xs:documentation> Fill to required quantity (REQQTY) when available stock is lower than
            minimum quantity (MINQTY).</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="MAX-WARN">
            <xs:annotation>
              <xs:documentation> Fill to maximum quantity (MAXQTY) when available stock is lower than
            warn quantity (WARNQTY).</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:schema>
