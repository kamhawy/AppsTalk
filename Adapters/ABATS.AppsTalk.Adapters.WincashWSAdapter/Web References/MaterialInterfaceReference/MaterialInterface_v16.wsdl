            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="updateStamp" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The updateStamp contains the last point of time this Record has been
            modified. The value will be automatically set on save of this Record.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="insertUserNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The insertUserNumber is used to identify the Employee who has created
              this data record. It references EMPLOYEE.EMPNO. The value will be automatically set on
              insert of this Record.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="insertUserID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The insertUserID is an internally used ID to identify the Employee who
              has created this data record. It references EMPLOYEE.GID. The value will be
              automatically set on insert of this Record.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="insertStamp" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The insertStamp contains the point of time this Record has been created.
            The value will be automatically set on insert of this Record.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="parameterList">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="parameter" type="nts:parameter" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="parameter" type="nts:parameter" />
      <xs:complexType name="parameter">
        <xs:sequence>
          <xs:element name="key" type="xs:string" />
          <xs:element name="value" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://jif.ntswincash.com/common/adapter/v12" jaxb:extensionBindingPrefixes="xjc" jaxb:version="2.0" />
    <xs:schema xmlns:m2s="http://jif.ntswincash.com/entity/materialToStockMapping/v12" xmlns:cusval="http://jif.ntswincash.com/common/customvalues/v12" xmlns:msu="http://jif.ntswincash.com/entity/materialToSupplier/v12" xmlns:m2dds="http://jif.ntswincash.com/entity/materialToDirectDeliverySource/v12" xmlns:m2f="http://jif.ntswincash.com/entity/materialToFeature/v12" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:ntsmas="http://jif.ntswincash.com/common/basetypes/masterdata/v12" xmlns:mat="http://jif.ntswincash.com/entity/material/v14" xmlns:mst="http://jif.ntswincash.com/entity/materialSetComponent/v12" xmlns:mpp="http://jif.ntswincash.com/entity/pricePlan/v12" xmlns:mbc="http://jif.ntswincash.com/entity/materialBarcode/v12" xmlns:nts="http://jif.ntswincash.com/common/basetypes/v12" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://jif.ntswincash.com/entity/material/v14" jaxb:version="2.0">
      <xs:import namespace="http://jif.ntswincash.com/common/basetypes/v12" />
      <xs:import namespace="http://jif.ntswincash.com/common/basetypes/masterdata/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialToStockMapping/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialToSupplier/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialBarcode/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialToFeature/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialSetComponent/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/materialToDirectDeliverySource/v12" />
      <xs:import namespace="http://jif.ntswincash.com/entity/pricePlan/v12" />
      <xs:import namespace="http://jif.ntswincash.com/common/customvalues/v12" />
      <xs:element name="materials" type="mat:standardMaterialList" />
      <xs:element name="setMaterials" type="mat:setMaterialList" />
      <xs:element name="setMaterial" type="mat:setMaterial" />
      <xs:element name="giftVoucherMaterials" type="mat:giftVoucherMaterialList" />
      <xs:element name="giftVoucherMaterial" type="mat:giftVoucherMaterial" />
      <xs:element name="eLoadingMaterials" type="mat:eLoadingMaterialList" />
      <xs:element name="eLoadingMaterial" type="mat:eLoadingMaterial" />
      <xs:element name="iTunesMaterials" type="mat:iTunesMaterialList" />
      <xs:element name="iTunesMaterial" type="mat:iTunesMaterial" />
      <xs:complexType name="standardMaterialList">
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecordList">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="material" type="mat:standardMaterial" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="setMaterialList">
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecordList">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="material" type="mat:setMaterial" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="giftVoucherMaterialList">
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecordList">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="material" type="mat:giftVoucherMaterial" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="eLoadingMaterialList">
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecordList">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="material" type="mat:eLoadingMaterial" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="iTunesMaterialList">
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:masterDataRecordList">
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="material" type="mat:iTunesMaterial" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="setMaterial">
        <xs:annotation>
          <xs:documentation>Contains in addition to the standard material settings the definition of the
        set type and set component list.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="mat:standardMaterial">
            <xs:sequence>
              <xs:element minOccurs="0" name="setHandling" type="mat:setHandling">
                <xs:annotation>
                  <xs:documentation>If the material is a set item, the type of the set (sales set or
                bill of goods) and the set components need to be defined</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="giftVoucherMaterial">
        <xs:annotation>
          <xs:documentation>Contains in addition to the standard material settings the definition of
        gift voucher settings (printed/not printed gift voucher, print layout, check against voucher
        list, payment assignment...).</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="mat:standardMaterial">
            <xs:sequence>
              <xs:element minOccurs="0" name="giftVoucherSettings" type="mat:giftVoucherSettings">
                <xs:annotation>
                  <xs:documentation>Gift voucher dependent information</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="eLoadingMaterial">
        <xs:annotation>
          <xs:documentation>This type is used to differentiate between a standard article and an
        eLoading article (eVoucher/eTopup article). If this type is sent, the interface
        automatically generates correct article type settings.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="mat:standardMaterial" />
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="iTunesMaterial">
        <xs:annotation>
          <xs:documentation>This type is used to differentiate between a standard article and an iTunes
        article. If this type is sent, the interface automatically generates correct article type
        settings.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="mat:standardMaterial" />
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="standardMaterial">
        <xs:annotation>
          <xs:documentation>An NTS Retail material record. Contains general information, prices, several
        flags for sales/logistics business cases, inventory handling data, serial number handling,
        barcodes, set component assignments, ...</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:mainImportRecordInheritable">
            <xs:sequence>
              <xs:element minOccurs="0" name="generalSettings" type="mat:generalSettings">
                <xs:annotation>
                  <xs:documentation>General article information like material number and name, material
                group and goods group references, unit of measure, ...</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="priceSettings" type="mat:priceSettings">
                <xs:annotation>
                  <xs:documentation>All information regarding material price: retail price, tax
                information, currency, price list handling, reduction handling, ...
              </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="serialSettings" type="mat:serialSettings">
                <xs:annotation>
                  <xs:documentation>defines if a serial number is mandatory, if the serial is tracked in
                the serial number pool, and a list of serial number definitions. The serial number
                count (column MATERIAL.SERCNT) is filled automatically by the interface depending on
                the number of definitions contained in the list.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" ref="mbc:materialBarcodes">
                <xs:annotation>
                  <xs:documentation>List of barcodes for this material</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="inventoryHandling" type="mat:inventoryHandling">
                <xs:annotation>
                  <xs:documentation>Information if the material is stock controlled, assignments to
                inventory locations and suppliers, ...</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="extendedSettings" type="mat:extendedSettings">
                <xs:annotation>
                  <xs:documentation>Usage information, reservation settings, lease settings, warranty
                settings, ...</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="featureSettings" type="mat:featureSettings">
                <xs:annotation>
                  <xs:documentation>Defines a list of features available for this material and how the
                feature selection is performed in NTS sales</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="directDeliverySettings" type="mat:directDeliverySettings">
                <xs:annotation>
                  <xs:documentation>Information about the direct delivery configuration of an article.
                This describes if direct delivery is enabled, if only direct delivery is possible
                and a list of available direct delivery sources.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="materialNumber" type="xs:string" use="required">
              <xs:annotation>
                <xs:documentation>The materialNumber represents an alphanumeric identifier for this
              Material. It must be unique among all Materials and must not be null.
            </xs:documentation>
              </xs:annotation>
            </xs:attribute>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="directDeliverySettings">
        <xs:annotation>
          <xs:documentation>Information about the direct delivery configuration of an article. This
        describes if direct delivery is enabled, if only direct delivery is possible and a list of
        available direct delivery sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="enableDirectDelivery" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Indicates if direct delivery is allowed for article.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="onlyDirectDeliveryAvailable" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Indicates that article is only available for direct delivery.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="m2dds:materialToDirectDeliverySources">
            <xs:annotation>
              <xs:documentation>Contains a list of direct delivery sources.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:element name="materialNameTranslations" type="ntsmas:translationList" />
      <xs:element name="materialName2Translations" type="ntsmas:translationList" />
      <xs:complexType name="generalSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="materialName" type="xs:string">
            <xs:annotation>
              <xs:documentation>The materialName contains the name of this Material. This value can not
            be null.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="mat:materialNameTranslations">
            <xs:annotation>
              <xs:documentation>Translations for the material name. Defined by a translation item
            (containing language and translated text) for each language.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="materialName2" type="xs:string">
            <xs:annotation>
              <xs:documentation>The materialName2 contains the second name of this Material.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="mat:materialName2Translations">
            <xs:annotation>
              <xs:documentation>Translations for the material name2. Defined by a translation item
            (containing language and translated text) for each language.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="goodsGroupName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The goodsGroupName is used to identify the goods group this Material
              belongs to. It references WAREGROUP.WGNAM.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="goodsGroupID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The goodsGroupID is an internally used ID to identify the goods group
              this Material belongs to. It references WAREGROUP.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:choice minOccurs="0">
            <xs:element name="materialGroupName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The materialGroupName is used to identify the MaterialGroup this
              Material belongs to. It references MATGROUP.MGNAM.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="materialGroupID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The materialGroupID is an internally used ID to identify the
              MaterialGroup this Material belongs to. It references MATGROUP.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="sortMaterialNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The sortMaterialNumber contains the right-aligned materialNumber and is
            used for sorting. The string is filled with whitespaces on the left to a fixed length of
            22 characters.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="supplierMaterialNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The supplierMaterialNumber contains another material number which acts
            as a second, optional, alphanumeric identification string.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="sortSupplierMaterialNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The sortSupplierMaterialNumber stores the right-aligned
            supplierMaterialNumber which is used for sorting. The string is filled with whitespaces
            on the left to a fixed length of 22 characters.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="unitName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The unitName is used to identify the Unit of this Material. It
              references MATUNIT.UNITNAM.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="unitID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The unitID is an internally used ID to identify the Unit of this
              Material. It references MATUNIT.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:choice minOccurs="0">
            <xs:element name="manufacturerName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The manufacturerName identifies the manufacturer that manufactured the
              material. It references MANUFACTURER.MANUNO
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="manufacturerID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The manufacturerID identifies the manufacturer that manufactured the
              material. It references MANUFACTURER.GID
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="isRelevantForConsulting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines if this material is relevant for sales assistance. Default value is 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="keyWords" type="mat:keyWordList">
            <xs:annotation>
              <xs:documentation>This element contains the keywords that describe the material. Those
            keywords are additionally used for searching when TSYSTEM.USEKEYWORDSEARCH='Y'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="keyWordList">
        <xs:annotation>
          <xs:documentation>This type contains a list of key words.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="keyWord" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="priceSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="price" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The price stores the price of this article. If isNettoPrice = 'Y' this
            attribute contains the net price, else this attribute contains the gross price.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="suggestedPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The suggestedPrice is an information field for this Material which
            contains the suggested sales price.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="periodicFee" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Defines the periodic fee of an article. Can be defined 
          in addition or instead of standard retail price.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="currencyName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The currencyName is used to identify the Currency which is used for
              all price fields in this Material. It references TCURRENCY.CURRSNAM.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="currencyID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The currencyID is an internally used ID to identify the Currency which
              is used for all price fields in this Material. It references TCURRENCY.GID.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="timeDependingPriceSettings" type="mpp:timeDependingPriceSettings">
            <xs:annotation>
              <xs:documentation>This element is able to defines time depending price plans.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="taxNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The taxNumber is used to identify the tax group for this article. It
              references TAXGROUP.TGNO.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="taxID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The taxID is an internally used ID to identify the tax group for this
              article. It references TAXGROUP.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="isNettoPrice" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The isNettoPrice flag specifies if the article price (see price) is an
            net price.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyPriceOverwriting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyPriceOverwriting flag indicates whether the price of this
            Material is allowed to be overwritten or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="priceListsAvailable" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The priceListsAvailable flag indicates whether price lists are available
            for this Material. If 'Yes', more material prices will be retrieved from the MATPRICE
            table. If 'No', no price lists will be loaded for this article (also if there would be
            valid price conditions for the article configured they will be ignored)
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="showPopupList" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The showPopupList flag indicates if the price list selection dialog is
            shown when the article is loaded in NTS sales. It is only used
            when priceListsAvailable = 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="popupPriceListOnlyExisting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The popupPriceListOnlyExisting flag indicates if only price lists, which
            contain a price for this Material, should be shown in the price list selection dialog.
            It is only used when priceListsAvailable = 'Yes' and showPopupList = 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="displayDefaultPriceInPriceList" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The displayDefaultPriceInPriceList flag indicates whether the default
            price should be shown in the price list selection dialog in NTS sales. 
            It is only used when priceListsAvailable = 'Yes' and showPopupList = 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="standardPriceListTypeNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The standardPriceListTypeNumber is used to identify the default
              PriceListType of this Material. It references PRICELISTTYPE.PLTYPENO. It defines which
              price list is selected by default in the price list selection dialog in NTS
              sales.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="standardPriceListTypeID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The standardPriceListTypeID is an internally used ID to identify the
              default PriceListType of this Material. It references PRICELISTTYPE.GID. It defines
              which price list is selected by default in the price list selection dialog in NTS
              sales.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="denyReduction" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyReduction flag indicates if it is possible to reduct the price
            of this Material or not. If 'Yes', the price can not be reduced by manual reductions or
            price lists which are handled as discounts.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="minimumPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The minimumPrice represents the minimum price this Material can be sold
            for. When a reduction is applied which would set the price below the minimum price, the
            minimumPrice will be used instead.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="maximumPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The maximumPrice specifies the maximum price for this Material.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="commission" type="nts:nonFractionNumber">
              <xs:annotation>
                <xs:documentation>The commission stores the provisioners commission in % of the sales
              price (price). Either commission or commissionAbsolute can be used.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="commissionAbsolute" type="nts:nonFractionNumber">
              <xs:annotation>
                <xs:documentation>The commissionAbsolute represents the provisioners commission as an
              absolute value. Either commission or commissionAbsolute can be used.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="turnoverRelevancy" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The turnoverRelevancy contains the percentage of turnover relevancy.
            When the item is sold, the turnover will be stored in VOUDTL.TURNOVERNET and
            VOUDTL.TURNOVERGROSS. The value must be between 0 and 100.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="noMarginInfluence" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The noMarginInfluence flag defines if this Material affects the margin
            or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="employeePrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The employeePrice contains the price that an employee would pay for this
            Material. This price is shown in the article detail dialog in NTS sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="allowNegativeReducedPrice" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Allows discounts and price lists to lower the reduced price to less than
            zero. Default value is 'No'.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="serialSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="forceSerial" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The forceSerial flag indicates whether it is mandatory to enter a serial
            number for this article within a transaction or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="serialPoolTracked" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The serialPoolTracked flag indicates whether this Material is a serial
            number guided article. The serial numbers for this Material are stored in the table
            MATSERPOOL and this article can only be sold if a serial number is available.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="mat:serialNumberDefinitions">
            <xs:annotation>
              <xs:documentation>List of serial numbers for this material</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="inventoryHandling">
        <xs:sequence>
          <xs:element minOccurs="0" name="stockControl" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The stockControl flag indicates whether item storage tracking is
            activated or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyNegativeStockQuantity" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyNegativeStockQuantity flag indicates if negative stock
            quantities for this Material are denied. If 'Yes', a sale is not possible if the stock
            quantity would become negative. It is only used when stockControl = 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="costPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The costPrice stores the cost price of this Material. The costPrice is
            not relevant for any sales transaction.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="averageCostPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The averageCostPrice contains the average cost price calculated by NTS
            logistics at every goods admission. This value is not relevant for any sales
            transaction.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="valuationPrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The valuationPrice contains the price of this Material for inventory
            valuation. Can also be set for each MAT2STOCKMAPPING record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAutomaticOrder" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyAutomaticOrder flag indicates if automatic order is denied. If
            'Yes', no automatic order templates will be generated for this Material.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="useStockReportValue" type="nts:flag">
            <xs:annotation>
              <xs:documentation> The useStockReportValue flag indicates if the value of the
            stockReportValue attribute should be used for stock reports.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="stockReportValue" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The stockReportValue contains the value for stock reports. It is only
            used when useStockReportValue = 'Y'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="boxCount" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The boxCount stores the number of items in a box.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="DRFSubsequentMaterialNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The subsequent article for dynamic replenishment forecast (DRF) calculation.
              References MATERIAL.MATNO.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DRFSubsequentMaterialID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The subsequent article for dynamic replenishment forecast (DRF) calculation.
              References MATERIAL.GID.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="useForDRFCalculation" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines if the current article should be considered as predecessor article for
            dynamic replenishment forecast (DRF) calculation. Can only be activated if a subsequent DRF
            article is defined.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="m2s:inventoryAssignments">
            <xs:annotation>
              <xs:documentation>List of inventory locations where this material should be available
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="msu:supplierAssignments">
            <xs:annotation>
              <xs:documentation>List of suppliers which can deliver this material</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="extendedSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="usageSettings" type="mat:usageSettings">
            <xs:annotation>
              <xs:documentation>Usage settings can be e.g. if the item is denied for returns/automatic
            order/..., if the item can be sold stand alone or only as set component, if a reject
            reason is required at return, if customer fields are mandatory, ...</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="extendedMaterialDescription" type="mat:extendedMaterialDescription">
            <xs:annotation>
              <xs:documentation>Contains additional descriptions like product description of the vendor,
            picture of the material (filename of the picture) or a hyperlink to the vendor's
            homepage</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="warrantySettings" type="mat:warrantySettings">
            <xs:annotation>
              <xs:documentation>Several settings for material warranty</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="leaseSettings" type="mat:leaseSettings">
            <xs:annotation>
              <xs:documentation>Defines settings like minimum lease period, lease price, if an estimated
            return date is required, ...</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="reservationSettings" type="mat:reservationSettings">
            <xs:annotation>
              <xs:documentation>Several settings for material reservation handling</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="repairManagerSettings" type="mat:repairManagerSettings">
            <xs:annotation>
              <xs:documentation>This section contains settings releated to the NTS repairmanager.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="additionalInformation" type="mat:additionalInformation">
            <xs:annotation>
              <xs:documentation>Miscellaneous material settings, which are mostly not used in standard
            NTS Retail modules but contain additional information for reporting or interface purpose
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="repairManagerSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="isRepairable" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Repairable with NTS repairmanager. Default value is 'No'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="repairCost" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Contains the maximum amount that will be paid for the repair. This value
            is used by the repair manager.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="processCost" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Defines the process charge cost for the repair</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="repairCenterCost" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>Contains the maximum amount that will be paid for the repair. This value
            is transfered to the repair center.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="autoCompleteRepairOrderOnTransfer" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines whether the repair order containing this material is
            automatically completed when transferring it to the repair center.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="usePostageForRepairOrder" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines whether the repaired material is sent as mail per default. Default
            value is 'No'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="isRepairLoanable" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The article can be loaned while the customer item is repaired. Default
            value is 'No'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="isSwapMandatory" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines if the repair exchange is mandatory. Default value is 'No'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="repairHotlineSettings" type="mat:repairHotlineSettings">
            <xs:annotation>
              <xs:documentation>
            This defines how the customer can get support for repairs of the
            current article.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="repairHotlineSettings">
        <xs:sequence>
          <xs:annotation>
            <xs:documentation>This element defines how the customer can contact a support hotline for
          the current article. The default way to contact the support hotline is using the description
          of a terminal profile (Web Repairmanager / Settings / sContactCenterLocationNo).
        </xs:documentation>
          </xs:annotation>
          <xs:choice minOccurs="0">
            <xs:element name="terminalProfile" type="nts:flag">
              <xs:annotation>
                <xs:documentation>
              If this element is present (no matter which value is used), the hotline information
              is read from a terminal profile (Web Repairmanager / Settings / sContactCenterLocationNo).
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="freeText" type="xs:string">
              <xs:annotation>
                <xs:documentation>This element contains a free text which describes how the customer
              can contact the support hotline.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:choice>
              <xs:annotation>
                <xs:documentation>This choice can be used to reference a supplier, which's hotline
              should be contacted.
            </xs:documentation>
              </xs:annotation>
              <xs:element name="supplierName" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The supplier. References SUPPLIER.SUPNO.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="supplierID" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>The supplier. References SUPPLIER.GID.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
            <xs:choice>
              <xs:annotation>
                <xs:documentation>This choice can be used to reference a shop, which should be
              contacted for support. References to regions or to the global level are not allowed.
            </xs:documentation>
              </xs:annotation>
              <xs:element minOccurs="0" name="shopName" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The name of the shop to which this record belongs to. References
                SHOPTREE.NAME
              </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="treeID" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>The shop or shop folder this record belongs to. In shop databases
                the value is always 0.
              </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:choice>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="usageSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="rejectionReason" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The rejectionReason flag indicates if a return reason must be specified
            when this Material is returned.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="forceCustomerData" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The forceCustomerData flag indicates whether customer data should be
             forced or not. If 'Yes' the element 'mandatoryCustomerFields' contains the customer fields
             which are mandatory. If set to 'No' no customer fields are mandatory.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="mandatoryCustomerFields" type="mat:mandatoryCustomerFields">
            <xs:annotation>
              <xs:documentation>The mandatoryCustomerFields element contains an attribute for each
            configurable customer field. To define a customer field mandatory, the corresponding
            attribute has to be set to "true" in this element. The element has no value itself, only
            a list of attributes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="checkSetMember" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The checkSetMember flag indicates whether NTS sales
            opens a list of all sets that contain this article when it is loaded.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyEdit" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyEdit flag indicates whether the modification of this Material is
            denied or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyNameOverwriting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyNameOverwriting flag indicates whether the name of this Material
            is allowed to be overwritten by the Employee at sale or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAllTransactions" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyAllTransactions flag indicates if all transactions are denied.
            If 'Yes', this Material can not be used for any transaction.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denySale" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denySale flag indicates whether the sale of this Material is denied
            or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyReturn" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyReturn flag indicates whether the return of this Material is
            denied or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyExchange" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyExchange flag indicates if the exchange is denied for this
            Material or not. If 'Yes', no exchange transactions are allowed with this article.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyPostVoid" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyPostVoid flag indicates whether cancellations of transactions
            containing this Material are denied or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyOrder" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyOrder flag indicates whether the order of this Material is
            denied or not. If 'Yes', it is not possible to manually order this article using NTS
            logistics.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyGoodsAdmission" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyGoodsAdmission flag indicates whether goods admissions in NTS
            logistics should be denied or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyGoodsRemoval" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyGoodsAdmission flag indicates whether goods removals in NTS
            logistics should be denied or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyPhysicalCount" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyPhysicalCount flag indicates whether stocktaking is denied or
            not. If 'Yes', the article is not available for physical inventory.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyDisposition" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyDisposition flag indicates whether the disposition of this
            article should be denied in NTS logistics.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyWaitList" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyWaitList flag specifies if waitlist reservations for this
            Material should be denied.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyDynamicReplenishment" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyDynamicReplenishment flag indicates whether this article should be denied
            for dynamic replenishment forecasting in NTS logistics.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAsMainItem" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Determines if this material is denied to be registered as a main item.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAsProductionListItem" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Determines if this material is denied to be registered as a production
            list component item.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAsSetItem" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Determines if this material is denied to be registered as a set
            component item.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAsSuccessorItem" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Determines if this material is denied to be registered as subsequent
            item.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyAsSystemItem" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Determines if this material is denied to be used by the system e.g. by
            sales plug-ins.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyReturnDelivery" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyReturnDelivery flag indicates whether return delivery of this
            Material should be denied or not. Articles can not be sent back to central if the value
            is 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyBasketSale" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines whether the material is denied for basket transactions.
          Y=Yes, N=No, Default=N.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyStockReservation" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines whether the reservation via NTS stock reservation service 
          is denied. Y=Yes, N=No, Default=N.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="hasToBeWeighed" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The hasToBeWeighed flag indicates whether this article has to be weighed
            with a scale when it is sold.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="isCommission" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The isCommission flag indicates whether this Material belongs to a
            franchisor or not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="exclusive" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The exclusive flag defines if this article can only be used exclusive in
            a transaction. If set to 'Yes', no other items can be entered in same transaction.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="isUseUserItemRestriction" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Enable transaction based user rights for actual article. User rights are
            defined via property USRITEMRES.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="allowQuantityOverwriting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>If this flag is set the article quantity can be overwritten. However it
            can never be overwritten for vouchers and eLoading articles. Default value is 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="maximalSaleAmount" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>This value represents the maximum sales quantity of an article per
            transaction (must be positive).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="allowPaymentSplitting" type="nts:flag">
            <xs:annotation>
              <xs:documentation>Defines if the splitting of means of payment is allowed if this material is part 
          of the transaction. Default value is 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element ref="mat:restrictedPaymentIDs">
              <xs:annotation>
                <xs:documentation>List of IDs of means of payment which are allowed to use in transactions with 
            this article. If the list is empty, all means of payment are allowed.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="mat:restrictedPaymentNames">
              <xs:annotation>
                <xs:documentation>List of names of means of payment which are allowed to use in transactions with 
            this article. If the list is empty, all means of payment are allowed.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
      </xs:complexType>
      <xs:element name="materialDescriptionTranslations" type="ntsmas:translationList" />
      <xs:complexType name="extendedMaterialDescription">
        <xs:sequence>
          <xs:element minOccurs="0" name="materialDescription" type="xs:string">
            <xs:annotation>
              <xs:documentation>The materialDescription contains a description of this Material which is
            shown in the article detail dialog in NTS sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="mat:materialDescriptionTranslations">
            <xs:annotation>
              <xs:documentation>Translations for the material description. Defined by a translation item
            (containing language and translated text) for each language.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="materialPicture" type="xs:string">
            <xs:annotation>
              <xs:documentation>The materialPicture represents the name of a file in the local file
            system that contains a picture of the article. This picture is shown in the article
            detail dialog in NTS sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="hyperlink" type="xs:string">
            <xs:annotation>
              <xs:documentation>The hyperlink contains an internet link to an information page of this
            Material.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="warrantySettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="hasWarranty" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The hasWarranty flag indicates whether this Material has warranty or
            not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="warrantyPeriod" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The warrantyPeriod contains the warranty time of this Material in
            months. It is only used if hasWarranty = 'Y'. The warrantyPeriod is shown in the article
            detail dialog of NTS sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="warrantyText" type="xs:string">
            <xs:annotation>
              <xs:documentation>The warrantyText contains the warranty text of this Material.It is only
            used if hasWarranty = 'Yes'. The warrantyText is shown in the article detail dialog of
            NTS sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="canOverwriteWarrantyInformation" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The canOverwriteWarrantyInformation flag specifies if the warranty
            period specified in warrantyPeriod can be overwritten by the sales assistant in NTS
            sales.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="reservationSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="denyReservation" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyReservation flag indicates whether the reservation of this
            Material should be denied.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="advanceRatio" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The advanceRatio represents the percentage of the sales price a customer
            must pay in advance to reserve the item. The value must be between 0 and 100. It is only
            used when denyReservation = 'No'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="reservationDummy" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The reservationDummy flag indicates if this is a dummy article which is
            used for reservations of articles that don't exist at the time of reservation. If 'Yes',
            this Material can not be sold directly.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="leaseSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="canBeLeased" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The canBeLeased flag indicates whether this Material can be leased or
            not.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="minimumLeasePeriod" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The minimumLeasePeriod represents the minimum period this Material can
            be leased. The unit of the value must be defined in minimumLeaseUnitID.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="minimumLeaseUnitName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The minimumLeaseUnitName is used to identify the unit of the
              minimumLeasePeriod. It references MATUNIT.UNITNAM.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="minimumLeaseUnitID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The minimumLeaseUnitID is an internally used ID to identify the unit
              of the minimumLeasePeriod. It references MATUNIT.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="leasePrice" type="nts:nonFractionNumber">
            <xs:annotation>
              <xs:documentation>The leasePrice contains the lease price per day. It is only used when
            canBeLeased = 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="requiredEstimatedReturnDate" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The requiredEstimatedReturnDate flag specifies if the input of an
            estimated return date for lease transactions is required. It is only used when
            canBeLeased = 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyLeaseReturn" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyLeaseReturn flag indicates if the return of an leased Material
            should be denied. Leased articles can not be returned if the value is 'Yes'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="denyLeaseExchange" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The denyLeaseExchange flag indicates whether the exchange of a leased
            Material should be denied. If 'Yes', leased articles can not be exchanged.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="featureSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="hasFeatures" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The hasFeatures flag defines if material features should be used for
            this Material or not. If 'No', NTS sales won't use assigned
            features.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="popupFeatures" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The popupFeatures flag indicates whether the feature selection dialog
            should appear when the article is loaded into NTS sales. It is
            only used when hasFeatures = 'Yes'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="m2f:featureAssignments">
            <xs:annotation>
              <xs:documentation>List of available features for this material.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="giftVoucherSettings">
        <xs:sequence>
          <xs:element minOccurs="0" name="voucherType" type="mat:giftVoucherType">
            <xs:annotation>
              <xs:documentation>The voucherType defines if the gift voucher is printed by NTS
            sales or is available pre-printed in the shops (P/N).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="checkAgainstVoucherList" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The checkAgainstVoucherList flag defines if the gift voucher must be
            checked against the available gift voucher list. It is only used when voucherType = 'N'
            or 'P'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="canOverExpiredVouchers" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The canOverExpiredVouchers flag indicates if the acceptance of expired
            gift vouchers is allowed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:element name="paymentName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The paymentName is used to identify the Payment for gift voucher
              payments. It is only used when voucherType = 'N' or 'P'. It references PAYMENT.PMSNAM.
              Only Payment records with TYPE='G' can be assigned.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="paymentID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The paymentID is an internally used ID to identify the Payment for
              gift voucher payments. It is only used when voucherType = 'N' or 'P'. It references
              PAYMENT.GID. Only Payment records with TYPE='G' can be assigned.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:choice minOccurs="0">
            <xs:element name="giftVoucherLayoutName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The giftVoucherLayout contains the print command. It references
              EXTPRTCMD.NAME. This command is used for gift vouchers (voucherType = 'P' or 'N') or
              for other types, when the printTextVoucher attribute is used.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="giftVoucherLayoutID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The giftVoucherLayout contains the print command. It references
              EXTPRTCMD.GID. This command is used for gift vouchers (voucherType = 'P' or 'N') or
              for other types, when the printTextVoucher attribute is used.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="voucherValiditySettings" type="mat:voucherValiditySettings">
            <xs:annotation>
              <xs:documentation>This sub element defines the validity of the gift voucher.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="voucherValiditySettings">
        <xs:choice minOccurs="0">
          <xs:element minOccurs="0" name="validTime" type="xs:integer">
            <xs:annotation>
              <xs:documentation>The validTime specifies the validity of the gift voucher in days. It is
            only used when voucherType is 'N' or 'P'.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:sequence>
            <xs:element minOccurs="0" name="voucherValidDay" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The voucherValidDay represents the day of the end date of the validity
              of the gift voucher. It is only used when voucherType is 'P' or 'N'.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="voucherValidMonth" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The voucherValidMonth represents the month of the end date of the
              validity of the gift voucher. It is only used when voucherType is 'P' or 'N'.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="voucherValidYears" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The voucherValidYears represents the number of years added to the end
              date of the validity of the gift voucher. It is only used when voucherType is 'P' or 'N'.
            </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:choice>
        <xs:attribute name="isUnlimited" type="nts:flag" use="optional">
          <xs:annotation>
            <xs:documentation>If this attribute is set to 'Yes', the voucher will not expire. Any
          additional voucher expiration settings will be ignored. If this attribute is set to 'No'
          the expiration time is defined by element 'validTime' or the combination of elements 
          'voucherValidDay', 'voucherValidMonth' and 'voucherValidYears'.
        </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:complexType>
      <xs:complexType name="additionalInformation">
        <xs:sequence>
          <xs:choice minOccurs="0">
            <xs:element name="accountNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The account number is used to identify the Account associated to this
              Material. This attribute has only informative character and can be used for report
              printing, but it has no relevance for sales and logistics transactions. It references
              ACCOUNT.ACNO.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="accountName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The account name/code is used to identify the Account associated to
              this Material. This attribute has only informative character and can be used for
              report printing, but it has no relevance for sales and logistics transactions. It
              references ACCOUNT.ACSNAM.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="accountID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The accountID is an internally used ID to identify the Account
              associated to this Material. This attribute has only informative character and can be
              used for report printing, but it has no relevance for sales and logistics
              transactions. It references ACCOUNT.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:choice minOccurs="0">
            <xs:element name="costCenterNumber" type="xs:string">
              <xs:annotation>
                <xs:documentation>The costCenterName is used to identify the CostCenter associated to
              this Material. This attribute has only informative character and can be used for
              report printing, but it has no relevance for sales and logistics transactions. It
              references COSTCENTER.COSTNO.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="costCenterID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The costID is an internally used ID to identify the CostCenter
              associated to this Material. This attribute has only informative character and can be
              used for report printing, but it has no relevance for sales and logistics
              transactions. It references COSTCENTER.GID.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="discontinued" type="nts:flag">
            <xs:annotation>
              <xs:documentation>The discontinued flag means that this Material won't be ordered any more
            if it is sold out (when value is 'Yes').</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="additionalText1" type="xs:string">
            <xs:annotation>
              <xs:documentation>The additionalText1 contains the additional text 1. It is a user defined
            value for use by external programs or for information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="additionalText2" type="xs:string">
            <xs:annotation>
              <xs:documentation>The additionalText2 contains the additional text 2. It is a user defined
            value for use by external programs or for information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="additionalText3" type="xs:string">
            <xs:annotation>
              <xs:documentation>The additionalText3 contains the additional text 3. It is a user defined
            value for use by external programs or for information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="externalOrderNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The externalOrderNumber contains an external order number of this
            Material.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="setType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="N">
            <xs:annotation>
              <xs:documentation>The article is not a set</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="S">
            <xs:annotation>
              <xs:documentation>The article is a sales set</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="P">
            <xs:annotation>
              <xs:documentation>The article is a production set (Bill of Goods)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="giftVoucherType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="P">
            <xs:annotation>
              <xs:documentation>Gift voucher is printed directly by NTS sales</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="N">
            <xs:annotation>
              <xs:documentation>Gift voucher is not printed by NTS sales (already available
            pre-printed in the shops)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="mandatoryCustomerFields">
        <xs:attribute name="customerNumber" type="nts:flag" use="optional" />
        <xs:attribute name="customerGroup" type="nts:flag" use="optional" />
        <xs:attribute name="salutation" type="nts:flag" use="optional" />
        <xs:attribute name="title" type="nts:flag" use="optional" />
        <xs:attribute name="name1" type="nts:flag" use="optional" />
        <xs:attribute name="name2" type="nts:flag" use="optional" />
        <xs:attribute name="street" type="nts:flag" use="optional" />
        <xs:attribute name="country" type="nts:flag" use="optional" />
        <xs:attribute name="ZIP" type="nts:flag" use="optional" />
        <xs:attribute name="city" type="nts:flag" use="optional" />
        <xs:attribute name="phone" type="nts:flag" use="optional" />
        <xs:attribute name="VATRegistrationNumber" type="nts:flag" use="optional" />
        <xs:attribute name="TIN" type="nts:flag" use="optional" />
        <xs:attribute name="company" type="nts:flag" use="optional" />
        <xs:attribute name="email" type="nts:flag" use="optional" />
        <xs:attribute name="email2" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField1" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField2" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField3" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField4" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField5" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField6" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField7" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField8" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField9" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField10" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField11" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField12" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField13" type="nts:flag" use="optional" />
        <xs:attribute name="additionalField14" type="nts:flag" use="optional" />
      </xs:complexType>
      <xs:element name="serialNumberDefinitions" type="mat:serialNumberDefinitionList" />
      <xs:complexType name="serialNumberDefinitionList">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="mat:serialNumberDefinition" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="serialNumberDefinition" type="mat:serialNumberDefinition" />
      <xs:complexType name="serialNumberDefinition">
        <xs:annotation>
          <xs:documentation>Definition of a serial number for a material</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="serialLength" type="xs:integer">
            <xs:annotation>
              <xs:documentation>The serialLength contains the number of characters per serial number.
            When the value is 0, the serial number can have any length.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="serialType" type="mat:serialType">
            <xs:annotation>
              <xs:documentation>The serialType represents the type of the serial numbers. Following
            serial types can be applied: SIM; CASH = CashCard; IMEI; RAND = Number is created
            randomly; CONS = Consecutive; 7812 = Iso7812. The number is validated as defined in ISO
            7812; EAN13 = Last digit is an automatically generated checksum; NONE = None
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="serialName" type="xs:string">
            <xs:annotation>
              <xs:documentation>The serialName represents the name of a serial number.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="serialFormat" type="xs:string">
            <xs:annotation>
              <xs:documentation>The serialFormat specifies a predefined format of valid serial numbers.
            It is only used in REPAIRManager(R). Each serial number can have multiple format string
            separated by ;. A format string contains a character string followed by : and the
            position number. A serial number will be valid when at leased on of the character
            strings is on the position specified by the position number.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="serialNumberValidation" type="xs:string">
            <xs:annotation>
              <xs:documentation>The serialNumberValidation contains an internal validation function for
            serial numbers.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="cusval:customValues">
            <xs:annotation>
              <xs:documentation>Optional customer specific values which are used to extends the
            interface.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:element name="restrictedPaymentIDs" type="mat:restrictedPaymentIDs" />
      <xs:complexType name="restrictedPaymentIDs">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="mat:paymentID" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="paymentID" type="xs:integer">
        <xs:annotation>
          <xs:documentation>This element stores the ID of a mean of payment. It references PAYMENT.GID
      </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="restrictedPaymentNames" type="mat:restrictedPaymentNameList" />
      <xs:complexType name="restrictedPaymentNameList">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="mat:paymentName" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="paymentName" type="xs:string">
        <xs:annotation>
          <xs:documentation>This element stores the name of a mean of payment. It refrences PAYMENT.PMSNAM
      </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:complexType name="setHandling">
        <xs:sequence>
          <xs:element minOccurs="0" name="setType" type="mat:setType" />
          <xs:element minOccurs="0" ref="mst:setComponents">
            <xs:annotation>
              <xs:documentation>List of set components for this material</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="serialType">
        <xs:annotation>
          <xs:documentation>The serialType represents the type of the serial numbers. Following serial
        types can be applied: SIM; CASH = CashCard; IMEI; RAND = Number is created randomly; CONS =
        Consecutive; 7812 = Iso7812. The number is validated as defined in ISO 7812; EAN13 = Last
        digit is an automatically generated checksum; NONE = None</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="NONE">
            <xs:annotation>
              <xs:documentation>None - serial number can be any value matching the additional validation
            settings (length, validation fucntion) defined for the serial number</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="SIM">
            <xs:annotation>
              <xs:documentation>SIM - Serial number is checked for validity as defined in ISO 7812
            (after predefined prefix is added to the actual sim number). The length of the serial
            number needs to be configured with 12 digits for this type</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CASH">
            <xs:annotation>
              <xs:documentation>Cash Card - The length of the serial number needs to be configured with
            12 digits for this type. No further checks are performed at validation of this serial
            number type.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="IMEI">
            <xs:annotation>
              <xs:documentation>IMEI - Serial number is checked for validity as defined in ISO 7812. The
            length of the serial number needs to be configured with 15 digits for this type
          </xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="RAND">
            <xs:annotation>
              <xs:documentation>Serial number is generated as (unique) random value when the article is
            sold in NTS sales</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CONS">
            <xs:annotation>
              <xs:documentation>Serial number is a (unique) consecutive number generated by NTS
            sales when the article is sold</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="7812">
            <xs:annotation>
              <xs:documentation>Serial number is validated as defined in ISO 7812. The length of the
            serial number needs to be configured with 15 digits for this type</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="EAN13">
            <xs:annotation>
              <xs:documentation>Last digit is an automatically generated checksum</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="ITUNES">
            <xs:annotation>
              <xs:documentation>This special serial number type used by iTunes. The serial number length
            is always 16 characters.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:schema>
    <xs:schema xmlns:nts="http://jif.ntswincash.com/common/basetypes/v12" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:ntsmas="http://jif.ntswincash.com/common/basetypes/masterdata/v12" xmlns:cusval="http://jif.ntswincash.com/common/customvalues/v12" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://jif.ntswincash.com/common/basetypes/masterdata/v12" jaxb:version="2.0">
      <xs:import namespace="http://jif.ntswincash.com/common/basetypes/v12" />
      <xs:import namespace="http://jif.ntswincash.com/common/customvalues/v12" />
      <xs:simpleType name="status">
        <xs:annotation>
          <xs:documentation>The status type represents the status of a master data record. Possible
        values for this type are "A" for active, "I" for inactive and "D" for deleted.
      </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="A">
            <xs:annotation>
              <xs:documentation>Active: visible and useable</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="I">
            <xs:annotation>
              <xs:documentation>Inactive: visible but not useable</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="D">
            <xs:annotation>
              <xs:documentation>Deleted: not visible or usable</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="translationList">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ntsmas:translation" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="translation" type="ntsmas:translation" />
      <xs:complexType name="translation">
        <xs:annotation>
          <xs:documentation>A translation element can be used to define translations for user-created
        master data records, if it is required to maintain the master data in more than 1 language.
        This can be for example a material description or the name of a reduction type which is
        printed on the receipt. A translation always contains a language and the translated text in
        this language.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="translatedLanguage" type="xs:string">
            <xs:annotation>
              <xs:documentation>Code of the language for the translation. Needs to match code defined
            for this language in central backoffice</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="translatedText" type="xs:string">
            <xs:annotation>
              <xs:documentation>The text translated to the defined language.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="mainImportRecord">
        <xs:annotation>
          <xs:documentation>The main import record type extends the inheritable record by an attribute
        which defines the last modification stamp of the record in the backend system.
      </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" ref="cusval:customValues">
            <xs:annotation>
              <xs:documentation> Optional customer specific values which are used to extends the
            interface.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="modificationStamp" type="xs:dateTime" use="optional">
          <xs:annotation>
            <xs:documentation>Timestamp of last modification of the record in the backend system. The
          time stamp is used to indicate if an update of the record in the NTS Retail database has
          to be performed. It is compared to the time stamp when the record was last imported to the
          NTS Retail database (import stamp is stored for each master data record).
        </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:complexType>
      <xs:complexType name="mainImportRecordInheritable">
        <xs:annotation>
          <xs:documentation>The main import record type extends the inheritable record by an attribute
        which defines the last modification stamp of the record in the backend system.
      </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
          <xs:extension base="ntsmas:mainImportRecord">
            <xs:sequence>
              <xs:element minOccurs="0" ref="ntsmas:inheritanceSettings" />
              <xs:element minOccurs="0" ref="ntsmas:masterDataSettings" />
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:element name="inheritanceSettings" type="ntsmas:inheritanceSettings">
        <xs:annotation>
          <xs:documentation>The inheritance attributes element contains a shop identifier and the
        identifier of the parent record (usually the identifier of the global record)
      </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:complexType name="inheritanceSettings">
        <xs:annotation>
          <xs:documentation>Each master data record in the NTS Retail system, which is inheritable (can
        have location-dependent settings) has a shop identifier and the identifier of the parent
        record (usually the identifier of the global record)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:choice>
            <xs:element minOccurs="0" name="shopName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The name of the shop to which this record belongs to. References
              SHOPTREE.NAME</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="treeID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The shop or shop folder this record belongs to. In shop databases the
              value is always 0. In central databases a value of 0 means, that the record is visible
              for all shops w/o assignments in table SHOPTREEDATA. A value greater than 0 is a
              reference to SHOPTREE.GID and means that this record is visible for this shop/shop
              folder and all its child shops/shop folders. There are also assignments in table
              SHOPTREEDATA, that assign the record to the shops/shop folders.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="parentID" type="xs:integer">
            <xs:annotation>
              <xs:documentation>If the value is -1 this is a root record and can be parent for other
            derivations.If the value is greater than -1 this record is a derivation and the value is
            a reference to the parent record (references MATFEATURE.GID). If the value is NULL, this
            is a root record that has no derivations and is visible for all shops. In shop databases
            the value is always NULL. For records with a value different to NULL, there are
            assignments in table SHOPTREEDATA, that assign the record to the shops/shop folders.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="isChild" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation>Defines if this master data record is a child record of another record. If
          yes, this record is automatically linked to it's parent record having the same identifier.
          Also only the values which are different to the parent record must be entered in this
          case.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:complexType>
      <xs:complexType name="masterDataRecord">
        <xs:annotation>
          <xs:documentation>The master data record type contains the master data settings (general ID,
        status, change control) of a master data record.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" ref="ntsmas:masterDataSettings" />
        </xs:sequence>
      </xs:complexType>
      <xs:element name="masterDataSettings" type="ntsmas:masterDataSettings">
        <xs:annotation>
          <xs:documentation>The master data attributes element contains the general ID (global
        identifier) of a record, it's status (active/inactive/deleted) and the change control
        information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:complexType name="masterDataSettings">
        <xs:annotation>
          <xs:documentation>Each master data record in the NTS Retail system has a general ID (global
        identifier), a status (active/inactive/deleted) and change control information.
      </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="generalID" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Unique number for internal identification. Values for new rows are
            calculated by a sequence or from table TINDEX. When the record is a derivation (PARENTID
            &gt; -1), the GID can not be used as foreign key. The GID of the root record must be used
            instead. For all NTS Retail standard import interfaces, the generalID will be ignored if
            a value is sent, because the internal GID has to be generated properly by the interface
            to avoid inconsistencies/unique constraint violations.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="status" type="ntsmas:status">
            <xs:annotation>
              <xs:documentation>The state of the record. A=Active: visible and useable. Constant:
            STATE_Active.I=Inactive: visible but not useable. Constant: STATE_Inactive. D=Deleted:
            not visible or usable.Constant: STATE_Deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="nts:changeControl" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="groupHierarchySettings">
        <xs:annotation>
          <xs:documentation>The group hierarchy type describes the hierarchy of a goods group or a ware
        group. Depending on the master element, the parent information references the table
        GOODSGROUP or MATERIALGROUP.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:choice minOccurs="0">
            <xs:element name="parentGroupName" type="xs:string">
              <xs:annotation>
                <xs:documentation>Parent of this goods group in the group hierarchy or NULL if it has no
              parent. References short name of parent record (GOODSGROUP or MATERIALGROUP)
            </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="treeParentID" type="xs:integer">
              <xs:annotation>
                <xs:documentation>Parent of this goods group in the group hierarchy or -1 if it has no
              parent. References GID of parent record. Default for write=-1.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:choice minOccurs="0">
            <xs:element minOccurs="0" name="groupDirectory" type="xs:string">
              <xs:annotation>
                <xs:documentation>Group hierarchy. The value is a list of all short names of parent
              records and the record itself seperated by '.', starting with the root record. The
              string is terminated with a '.' This value may be omitted or has to match with the
              parentGroupNames.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="treeDirectory" type="xs:string">
              <xs:annotation>
                <xs:documentation>Group hierarchy. The value is a list of all GIDs of parent records and
              the record itself seperated by '.', starting with the root record. The string is
              terminated with a '.' This value may be omitted or has to match with the
              treeParentIDs.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="treeLev" type="xs:integer">
            <xs:annotation>
              <xs:documentation>The level in the group hierarchy starting with 0. Default for write=0.
          </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="masterDataRecordList" abstract="true">
        <xs:annotation>
          <xs:documentation />
        </xs:annotation>
        <xs:attribute name="transferMode" type="ntsmas:dataTransferMode" use="optional">
          <xs:annotation>
            <xs:documentation>Default transfer mode is "DELTA" if the attribute is not sent.
        </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:complexType>
      <xs:simpleType name="dataTransferMode">
        <xs:restriction base="xs:string">
          <xs:enumeration value="DELTA">
            <xs:annotation>
              <xs:documentation>Delta transfer mode defines that only changed records are sent from the
            backend system to NTS Retail. This is the default setting if no transfer mode is
            explicitely defined for the import data.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="FULL">
            <xs:annotation>
              <xs:documentation>Full transfer mode defines that all existing records are sent from the
            backend system to NTS Retail. All records not contained in the import data are deleted
            from the NTS Retail database.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:schema>
